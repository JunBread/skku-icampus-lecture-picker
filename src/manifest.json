{
    "manifest_version": 2,
    "name": "SKKU iCampus Lecture Picker",
    "description": "Open video files in SKKU iCampus system",
    "version": "1.0.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "pages/popup.html",
        "default_title": "iCampus Lecture Picker"
    },
    "content_scripts": [ {
        "all_frames": true,
        "js": ["scripts/contentscript.js"],
        "matches": ["*://*.skku.edu/*"]
    }]
}